{
	"name": "VXCode Theme - Default",
	"semanticHighlighting": true,
	"colors": {
		"activityBar.activeBorder": "#505050",
		"activityBar.activeFocusBorder": "#505050",
		"activityBar.background": "#18191a",
		"activityBar.border": "#333333",
		"activityBar.foreground": "#ffffff",
		"activityBarBadge.background": "#199aff",
		"activityBarBadge.foreground": "#ffffff",
		"badge.background": "#199aff",
		"badge.foreground": "#ffffff",
		"button.background": "#555555",
		"button.foreground": "#fcfbfc",
		"button.hoverBackground": "#666666",
		"dropdown.background": "#18191a",
		"dropdown.border": "#333333",
		"editor.background": "#1c1d1e",
		"editor.foreground": "#ffffffd8",
		"editor.lineHighlightBackground": "#00000000",
		"editor.selectionBackground": "#4c566a",
		"editorGroup.border": "#404040",
		"editorGroupHeader.tabsBackground": "#18191a",
		"editorGroupHeader.tabsBorder": "#404040",
		"sideBar.background": "#18191a",
		"sideBar.border": "#404040",
		"sideBar.foreground": "#eaf0f6",
		"sideBarSectionHeader.background": "#18191a",
		"sideBarSectionHeader.border": "#404040",
		"statusBar.background": "#18191a",
		"statusBar.border": "#404040",
		"statusBar.foreground": "#ffffffd8",
		"statusBarItem.remoteBackground": "#fe7ab2",
		"statusBarItem.remoteForeground": "#ffffff",
		"statusBar.noFolderBackground": "#18191a",
		"statusBar.debuggingBackground": "#18191a",
		"tab.activeBackground": "#1c1d1e",
		"tab.activeBorderTop": "#fe7ab2",
		"tab.activeBorder": "#1c1d1e",
		"tab.inactiveBackground": "#18191a",
		"tab.inactiveForeground": "#b7b7b7",
		"titleBar.activeBackground": "#18191a",
		"titleBar.activeForeground": "#b8b7bc",
		"titleBar.border": "#404040",
		"panel.background": "#18191a",
		"panel.border": "#404040"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#7f8c98"
			}
		},
		{
			"name": "String",
			"scope": ["string"],
			"settings": {
				"foreground": "#fe8170"
			}
		},
		{
			"name": "Keyword and Storage",
			"scope": [
				"storage.type",
				"storage.modifier",
				"keyword",
				"variable.language.this"
			],
			"settings": {
				"foreground": "#fe7ab2"
			}
		},
		{
			"name": "Function Name",
			"scope": ["entity.name.function"],
			"settings": {
				"foreground": "#ffb454"
			}
		},
		{
			"name": "Class and Type",
			"scope": [
				"entity.name.type.class",
				"entity.other.inherited-class",
				"meta.return.type",
				"entity.name.type",
				"support.type.primitive"
			],
			"settings": {
				"foreground": "#82aaff"
			}
		},
		{
			"name": "Variables and Properties",
			"scope": [
				"variable.other.readwrite",
				"variable.object.property",
				"meta.object-literal.key",
				"variable.parameter",
				"variable.other.property",
				"support.type.property-name.json"
			],
			"settings": {
				"foreground": "#4ec9b0"
			}
		},
		{
			"name": "Constants and Booleans",
			"scope": [
				"variable.other.constant",
				"variable.other.enummember",
				"property.readonly",
				"constant.language.boolean"
			],
			"settings": {
				"foreground": "#ae81ff"
			}
		},
		{
			"name": "Key-Value Separator",
			"scope": [
				"punctuation.separator.key-value"
			],
			"settings": {
				"foreground": "#fe7ab2"
			}
		},
		{
			"name": "Numeric Constants",
			"scope": [
				"constant.numeric",
				"constant.numeric.integer",
				"constant.numeric.float",
				"constant.numeric.decimal"
			],
			"settings": {
				"foreground": "#7dcfff"
			}
		},
		{
			"name": "HTML Tags",
			"scope": [
				"entity.name.tag"
			],
			"settings": {
				"foreground": "#ae81ff"
			}
		},
		{
			"name": "HTML Attributes",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#4ec9b0"
			}
		},
		{
			"name": "Tag Punctuation",
			"scope": [
				"punctuation.definition.tag"
			],
			"settings": {
				"foreground": "#fe7ab2"
			}
		},
		{
			"name": "CSS Variables",
			"scope": [
				"variable.css",
				"variable.argument.css",
				"support.type.property-name.css",
				"variable.prop.css",
				"support.type.property-name.media.css",
				"variable.parameter.keyframe-list.css"
			],
			"settings": {
				"foreground": "#82aaff"
			}
		},
		{
			"name": "CSS Constants",
			"scope": [
				"support.constant.font-name.css",
				"support.constant.property-value.misc.css",
				"support.constant.property-value.css",
				"support.constant.media.css",
				"support.constant.color.w3c-standard-color-name.css",
				"support.constant.color.w3c-extended-color-name.css",
				"constant.other.color"
			],
			"settings": {
				"foreground": "#fe8170"
			}
		},
		{
			"name": "CSS Functions",
			"scope": [
				"support.function.misc.css"
			],
			"settings": {
				"foreground": "#ffb454"
			}
		},
		{
			"name": "Language Constants",
			"scope": [
				"constant.language"
			],
			"settings": {
				"foreground": "#ae81ff"
			}
		},
		{
			"name": "Markdown Headings",
			"scope": [
				"punctuation.definition.heading.markdown",
				"entity.name.section.markdown",
				"punctuation.definition.list.begin.markdown",
				"markup.bold.markdown"
			],
			"settings": {
				"foreground": "#ae81ff"
			}
		},
		{
			"name": "Markdown Inline Code",
			"scope": [
				"markup.inline.raw.string.markdown"
			],
			"settings": {
				"foreground": "#fe8170"
			}
		}
	]
}
